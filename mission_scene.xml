<mujoco>
  <compiler autolimits="true" assetdir="assets_hnu"/>
  <option solver="CG" tolerance="1e-6" timestep=".001"/>
  <!-- <include file="NURBS_coll.xml"/> -->

  <!-- <include file="hnuter.xml"/> -->
  <include file="hnuter_mission.xml" euler="0 0 0"/>
  <!-- <include file="hnuter203.xml" euler="0 0 0"/> -->
  <!-- <statistic center="0 -0.2 0.1" extent="1.0"/> -->
  <statistic center="0 0 1" extent="1.0"/>

  <asset>
    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>

  <default>
    <default class="car">
      <joint damping=".03" actuatorfrcrange="-0.5 0.5"/>
      <default class="wheel">
        <geom type="cylinder" size=".03 .01" rgba=".5 .5 1 1"/>
      </default>
      <default class="decor">
        <site type="box" rgba=".5 1 .5 1"/>
      </default>
    </default>
  </default>

  <worldbody>
    <!-- <light name="light1" pos="2 0 1" dir="1 0 1" diffuse="1 1 1" specular="0.7 0.7 0.7" /> -->
    <light name="light1" pos="0 0 10" dir="0 0 -1" diffuse="1 1 1" specular="0.7 0.7 0.7" />
    <geom name="floor" type="plane" size="0 0 0.05" material="grid"/>

    <body name="slope1" pos="5 0 0.25" euler="0 3.8 1.57"> 
      <geom type="box" size="5 5 0.05" rgba="0.7 0.3 0.3 1" mass="10"/>
    </body>

  </worldbody>
  

</mujoco>
